is_compat := AIs_compat : forall (c : field) (d : elt) (o : option elt'),
             deduce_middle_eval (c c') ->
              forall env : Env, eval' env t -> eval' env t \/ eval' env t
  a : (Term' * Annot)%type
  cl : list (Term' * Annot)
  IHcl : forall (cl' : clause) (env : Env),
         eval_opt_clause env (or_clause cl cl') <->
         ~ make_conj (eval_tt env) cl \/ ~ make_conj (eval_tt env) cl'
  cl' : clause
  env : Env
  c : clause
  H2 : eval_tt env a
  H3 : make_conj (eval_tt env) cl -> False
  H : make_conj (eval_tt env) c -> False
  H1 : eval_tt env a -> make_conj (eval_tt env) cl' -> False
  H0 : make_conj (eval_tt env) cl' -> False
  ============================
   (~ make_conj (eval_tt env) cl \/ ~ make_conj (eval_tt env) c)



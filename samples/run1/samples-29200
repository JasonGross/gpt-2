  Heq_r : (x :: _) = rpow (eps') n
  e1 : PExpr C
  e2 : PExpr C
  IHeq_x : (e1 - (e2 + 1) - (e2 <? n) = true -> ~ PExpr C) = true
  Heq_r : (x :: _) = rpow (eps) n
  r : R

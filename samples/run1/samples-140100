<Coq.Init.Tauto.is_disj> flags X1; elim id; intro


TcDebug (1) > 
Goal:
  
  elt : Type
  x : key
  d : elt
  r : t elt
  ll : t elt
  lx : key
  ld : elt
  lr : t elt
  _x : int
  l' : t elt
  m : (key * elt)%type
  e0 : remove_min ll lx ld lr = (l', m)
  IHp : forall (h : int) (y : key) (e' : elt),
        MapsTo y e' (Node ll lx ld lr h) <->
        X.eq y m#1 /\ e' = m#2 \/ MapsTo y e' l'
  h : int
  y : key
  e' : elt
  H : MapsTo y e' (Node ll lx ld lr _x) ->
      X.eq y m#1 /\ e' = m#2 \/ MapsTo y e' l'
  H0 : X.eq y m#1 /\ e' = m#2 \/ MapsTo y e' l' ->
      X.eq y m#1 /\ e' = m#2 \/ MapsTo y e' l'
  ============================
   (MapsTo y e' (Node l' x d r (max (height l') (height r) + 1)))



 t => make_conj (eval_tt env) cl
  H1 : eval' env t0
  H3 : make_conj (eval_tt env) cl -> False
  H2 : eval_tt env t
  H4 : make_conj (eval_tt env) cl' -> False
  ============================
   False


Debug: Going to execute:
<Coq.Init.Tauto.is_conj> flags X1; elim id


TcDebug (1) > 
Goal:
  
  Env : Type
  Term : Type
  Term' : Type
  Annot : Type
  unsat : Term' -> bool
  deduce : Term' -> Term' -> option Term'
  normalise : Term -> Annot -> cnf
  negate : Term -> Annot -> cnf
  eval' : Env -> Term' -> Prop
  no_middle_eval' : forall (env : Env) (d : Term'),
                    eval' env d \/ ~ eval' env d
  unsat_prop : forall t : Term',
               unsat t = true -> forall env : Env, eval' env t -> False
  deduce_prop : forall t t' u : Term',
                deduce t t' = Some u ->
                forall env : Env, eval' env t -> eval' env t' -> eval' env u
  env : Env
  t : (Term' * Annot)%type
  a : (Term' * Annot)%type
  cl : list (Term' * Annot)
  c : clause
  H : deduce (fst t) (fst a) = None
  H5 : eval_tt env a
  H6 : make_conj (eval_tt env) c
  H0 : make_conj (eval_tt env) c
  H1 : make_conj (eval_tt env) t
  H4 : make_conj (eval_tt env) cl
  H2 : eval_tt env t
  H3 : make_conj (eval_tt env) cl' -> False
  H2 : eval_tt env t
  H4 : make_conj (eval_tt env) cl'
  ============================
   False



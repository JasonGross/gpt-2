======== SAMPLE 1 ========
 (x : X.t) (y : elt),
        Ok (Bk rl rx rr) ->
        InT y (del x (Bk rl rx rr)) <->
        InT y (Bk rl rx rr) /\ (X.eq y x -> False)
  x : X.t
  y : elt
  H0 : X.lt x' x
  H7 : lt_tree x' l
  H8 : gt_tree x' (Bk rl rx rr)
  H2 : X.eq y x -> False
  H10 : Ok rl
  H11 : Ok rr
  H12 : lt_tree rx rl
  H13 : gt_tree rx rr
  H3 : InT y rl
  ============================
   (X.eq y rx \/
    InT y l \/ X.eq y x' \/ InT y rl \/ X.eq y rx \/ InT y rr)



======== SAMPLE 1 ========
       make_conj (eval_clause env)
          (fold_left
             (fun (acc0 : list clause) (e : clause) =>
              match or_clause t e with
              | Some cl => cl :: acc0
              | None => acc0
              end) f acc) <->
        (eval_clause env t \/ make_conj (eval_clause env) f) /\
        make_conj (eval_clause env) acc
  acc : list clause
  H0 : eval_clause env t
  H1 : True
  H4 : make_conj (eval_clause env) acc
  H3 : True
  H5 : eval_clause env t
  H : eval_clause env a
  H5 : make_conj (eval_clause env) f
  H2 : eval_clause env a
  ============================
   (eval_clause env a /\ make_conj (eval_clause env) acc)



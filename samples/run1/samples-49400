| |- context [ (?P -> False) -> ?Q -> False ] => rewrite ring_opp_zero
| |- context [ ?P -> ?Q -> False ] => rewrite ring_zero_eq
| |- context [ ?P /\ ?Q ] => rewrite ring_zero_opp_zero
| |- context [ (?P -> False) ? -> ?Q ]
| _ => idtac
end



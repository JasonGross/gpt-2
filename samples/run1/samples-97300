Goal:
  
  x : positive
  l1 : tree
  x1 : X.t
  l2 : tree
  x2 : X.t
  l : tree
  l' : list X.t
  H : false = true -> P x
  H0 : (forall x : elt, InT x l -> InT x l') /\
       (forall x : elt, InT x l -> InT x l')
  H2 : forall x : elt, InT x l -> InT x l
  ============================
   (forall x : negb (rev_var x) /\
    (forall x0 : X.t, InT x l -> X.lt x x0 -> i2z false x = true))



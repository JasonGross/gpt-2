                 unsat t = true -> forall env : Env, eval' env t -> False
  deduce_prop : forall t t' u : Term',
                 deduce t t' = Some u ->
                 forall env : Env, eval' env t -> eval' env t' -> eval' env u
  env : Env
  p : (Term' * Annot)%type
  a : (Term' * Annot)%type
  cl : list (Term' * Annot)
  c : clause
  H : deduce (fst p) (fst a) = None
  H0 : ~ make_conj (eval_tt env) c ->
       ~ (eval_tt env (p :: c) :: (_ /\ make_conj (eval_tt env) cl) ->
       ~ make_conj (eval_tt env) c
  H1 : ~ (eval_tt env a /\ make_conj (eval_tt env) c) ->
       ~ make_conj (eval_tt env) c
  H2 : ~ (eval_tt env a /\ make_conj (eval_tt env) cl)
  H3 : ~ (eval_tt env a /\ make_conj (eval_tt env) c) ->
       ~ make_conj (eval_tt env) c
  ============================
   (~ (eval_tt env a /\ make_conj (eval_tt env) c) ->
    ~ (eval_tt env a /\ make_conj (eval_tt env) cl))



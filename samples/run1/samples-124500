======== SAMPLE 1 ========
noDupA (l1 ++ l2)
  H2 : equivlistA (x :: l) (l1 ++ l2)
  a : A
  H3 : InA x l -> False
  H4 : NoDupA l
  H5 : NoDupA (l1 ++ l2)
  H7 : InA a l
  H8 : eqA a x -> False
  H9 : eqA a y -> False
  H6 : InA y l1 -> False
  H12 : InA y l2 -> False
  H0 : eqA a x -> InA a l1 \/ eqA a y \/ InA a l2
  H10 : InA a l2
  H13 : InA a l1 -> eqA a x \/ InA a l
  H11 : eqA a y -> eqA a x \/ InA a l
  H14 : eqA a x \/ InA a l
  ============================
   (eqA a x \/ InA a l)



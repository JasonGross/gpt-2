                     deduce t t' = Some u ->
                  forall env : Env, eval' env t -> eval' env t' -> eval' env u
  env : Env
  t : (Term' * Annot)%type
  a : (Term' * Annot)%type
  cl : list (Term' * Annot)
  c : clause
  t0 : Term'
  H : deduce (fst t) (fst a) = Some t0
  H0 : unsat t0 = false
  H1 : eval' env (fst t) -> eval' env (fst a) -> eval' env t0
  H2 : ~ make_conj (eval_tt env) c ->
       ~ (eval_tt env t /\ make_conj (eval_tt env) cl)
  H3 : ~ (eval_tt env t /\ make_conj (eval_tt env) cl) ->
       ~ make_conj (eval_tt env) c
  H4 : eval_tt env a /\ make_conj (eval_tt env) c -> False
  H5 : eval_tt env t /\ eval_tt env a /\ make_conj (eval_tt env) cl -> False
  ============================

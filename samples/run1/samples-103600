TcDebug (3) > 
Debug: Level 3: evaluation returns
<tactic closure>


Goal:
  
  A : Type
  eqA : A -> A -> Prop
  eqA_equiv : Equivalence eqA
  l : list A
  l1 : list A
  l2 : list A
  x : A
  y : A
  H : eqA x y
  H1 : NoDupA (l1 ++ y :: l2)
  H2 : equivlistA (x :: l) (l1 ++ y :: l2)
  a : A
  H3 : InA x l -> False
  H4 : NoDupA l
  H5 : NoDupA (l1 ++ l2)
  H6 : InA a l1
  H7 : InA y l1 -> False
  H10 : InA y l2 -> False
  H8 : InA a l
  H9 : InA a l2
  H0 : eqA a y -> eqA a x \/ InA a l
  H13 : eqA a x
  H12 : InA a l2
  ============================
   (eqA a x \/ InA a l)



TcDebug (1) > 
Goal:
  
  Env : Type
  Term : Type
  Term' : Type
  Annot : Type
  unsat : Term' -> bool
  deduce : Term' -> Term' -> option Term'
  normalise : Term -> Annot -> cnf
  negate : Term -> Annot -> cnf
  eval' : Env -> Term' -> Prop
  no_middle_eval' : forall (env : Env) (d : Term'),
                   eval' env d \/ ~ eval' env d
  unsat_prop : forall t : Term',
              unsat t = true -> forall env : Env, eval' env t -> False
  deduce_prop : forall t t' u : Term',
               deduce t t' = Some u ->
               forall env : Env, eval' env t -> eval' env t' -> eval' env u
  env : Env
  t : (Term' * Annot)%type
  a : (Term' * Annot)%type
  cl : list (Term' * Annot)
  t0 : Term'
  H : deduce (fst t) (fst a) = Some t0
  H0 : unsat t0 = true
  H3 : eval_opt_clause env (add_term t cl) -> eval_clause env (t :: cl)
  H4 : eval_clause env (t :: cl) -> eval_opt_clause env (add_term t cl)
  H5 : True
  H7 : eval_tt env t
  H6 : eval_tt env a
  H9 : make_conj (eval_tt env) cl
  H1 : eval' env t0
  H8 : False
  ============================
   False



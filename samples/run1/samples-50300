Goal:
  
  x : X.t
  x1 : X.t
  l1 : list X.t
  IH1 : forall (l2 : list X.t) (acc : list elt),
        Sorted X.lt (rev l1) ->
        Sorted X.lt (rev l2) ->
        InA X.eq x (diff_list l1 l2 acc) <->
        InA X.eq x l1 /\ ~ InA X.eq x l2 \/ InA X.eq x acc
  acc : list elt
  H : Sorted X.lt (rev (x1 :: s1))
  H0 : Sorted X.lt (rev nil)
  ============================
   (InA X.eq x acc)



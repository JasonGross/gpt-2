Goal:
  
  Env : Type
  Term : Type
  Term' : Type
  Annot : Type
  unsat : Term' -> bool
  deduce : Term' -> Term' -> option Term'
  normalise : Term -> Annot -> cnf
  negate : Term -> Annot -> cnf
  eval' : Env -> Term' -> Prop
  no_middle_eval' : forall (env : Env) (d : Term'),
                     eval' env d \/ ~ eval' env d
  unsat_prop : forall t : Term',
               unsat t = true -> forall env : Env, eval' env t -> False
  deduce_prop : forall t t' u : Term',
                deduce t t' = Some u ->
                forall env : Env, eval' env t -> eval' env t' -> eval' env u
  env : Env
  t : (Term' * Annot)%type
  a : (Term' * Annot)%type
  cl : list (Term' * Annot)
  H : deduce (fst t) (fst a) = None
  H0 : True
  H2 : True
  H4 : eval_tt env t
  H5 : eval_tt env a /\ make_conj (eval_tt env) cl
  H3 : eval' env t -> eval' env t /\ make_conj (eval_tt env) cl
  H6 : make_conj (eval_tt env) cl -> False

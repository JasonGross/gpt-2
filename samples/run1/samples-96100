  lmp : list (Mon * Pol)
  pe1 : Pol
  pe2 : Pol
  IHpe1 : npe1 > eval lmp n lmp ++ pe1
  IHpe2 : npe2 > eval lmp n (nhyps_of_psatz pe2))

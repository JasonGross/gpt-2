  s2 : tree
  IHt0 : forall y : elt,
         Ok l1 ->
         Ok (split x1 s2)#l ->
         InT y (union l1 (split x1 s2)#l) <->
         InT y l1 \/ InT y (split x1 s2)#l
  IHt1 : forall y : elt,
         Ok r1 ->
         Ok (split x1 s2)#r ->
         InT y (union r1 (split x1 s2)#r) <->
         InT y r1 \/ InT y (split x1 s2)#r

======== SAMPLE 1 ========
 env d \/ ~ eval' env d
  unsat_prop : forall t : Term',
                unsat t = true -> forall env : Env, eval' env t -> False
  deduce_prop : forall t t' u : Term',
                 deduce t t' = Some u ->
                 forall env : Env, eval' env t -> eval' env t' -> eval' env u
  env : Env
  t : (Term' * Annot)%type
  a : (Term' * Annot)%type
  cl : list (Term' * Annot)
  c : clause
  t0 : Term'
  H : deduce (fst t) (fst a) = Some t0
  H0 : ~ make_conj (eval_tt env) c ->
       ~ (eval_tt env t /\ make_conj (eval_tt env) cl)
  H4 : eval_tt env t
  H3 : eval_tt env a
  H6 : make_conj (eval_tt env) cl
  H2 : make_conj (eval_tt env) c
  ============================
   False



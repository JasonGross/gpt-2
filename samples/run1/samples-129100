         eval_opt_clause env (or_clause cl cl') <->
         ~ make_conj (eval_tt env) cl \/ ~ make_conj (eval_tt env) cl'
  cl' : clause
  env : Env
  c : clause
  H2 : eval_tt env a
  H : make_conj (eval_tt env) c -> False
  H1 : make_conj (eval_tt env) cl' -> False

Goal:
  
  A : Type
  tg : A -> A -> Prop
  a : A
  f : intuition A
  l : list A
  tg : A -> A -> Prop
  aa : A
  f_rw : f a = true
  H : f (f a) = true
  H0 : (f a = true) -> forall x : A, In x (filter (f a)) l <-> In x l /\ f x = true
  ============================
   ((f (f a) = true \/ In x (filter (f a)) l) /\ f x = true \/ In x l /\ f x = true)



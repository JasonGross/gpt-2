| NotConstant =>
    match t with
    | rO => fun _ => fv
    | rI => fun _ => fv
    | add ?t1 ?t2 => fun _ => TFV t2 ltac:(TFV t1 fv)
    | mul ?t1 ?t2 => fun _ => TFV t2 ltac:(TFV t1 fv)
    | sub ?t1 ?t2 => fun _ => TFV t2 ltac:(TFV t1 fv)

        Ok l -> InT y (del x l) <-> InT y l /\ ~ X.eq y x
  IHr : forall (x : X.t) (y : elt),
        Ok (Rd rl rx rr) ->
        InT y (del x (Rd rl rx rr)) <-> InT y (Rd rl rx rr) /\ ~ X.eq y x
  x : X.t
  y : elt
  H0 : X.lt x' x
  H5 : Ok l
  H6 : Ok (Rd rl rx rr)
  H7 : lt_tree x' l
  H8 : gt_tree x' (Rd rl rx rr)
  H : X.eq y x' \/ InT y l \/ InT y (Rd rl rx rr) /\ ~ X.eq y x
  ============================
